<!--pages/register/register.wxml-->

<view class="register">
  <van-cell-group>
    <van-field focus="{{true}}" model:value="{{ username }}" label="昵称" placeholder="请输入昵称"/>
    <van-field model:value="{{ phone }}" label="手机号" placeholder="请输入手机号" />
    <van-field model:value="{{ password }}" label="密码" placeholder="请输入密码" border="{{ false }}" />
  </van-cell-group>

  <van-button round type="info" bindtap="commit">注册</van-button>
</view>
