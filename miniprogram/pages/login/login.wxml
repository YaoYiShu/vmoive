<!--pages/login/login.wxml-->
<view class="login">
  <view class="title">使用“新片场账号”登录厂库</view>
  <van-cell-group>
    <van-field focus="{{true}}" model:value="{{ phone }}" label="手机号" placeholder="请输入手机号" />
    <van-field model:value="{{ password }}" type="password" label="密码" placeholder="请输入密码" border="{{ false }}" />
  </van-cell-group>

  <van-button round type="info" bindtap="login">登录</van-button>

  <view class="register" bindtap="toRegister">还没有账号？注册</view>

  <view class="view_fengexian">
    <view></view>
    <text class="text_fengexian">其他登录</text>
    <view></view>
  </view>
  <view class="other">
    <image mode="widthFix" src="../../assect/wechat.png"></image>
  </view>


</view>