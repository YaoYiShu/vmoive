/* components/channel/channel.wxss */
.channels {
  display: flex;
  width: 100%;
  flex-wrap: wrap;
}
.channels .channel {
  position: relative;
  width: 50%;
  display: flex;
  align-items: center;
  justify-content: center;
  flex-flow: wrap;
  box-sizing: border-box;
}

.channels .channel .catename {
  color: #fff;
  font-size: 14px;
  position: absolute;
}